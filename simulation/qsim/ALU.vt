`timescale 1 ps/ 1 ps
module ALU_vlg_sample_tst(
	A,
	B,
	buss,
	sampler_tx
);
input [3:0] A;
input [3:0] B;
input [2:0] buss;
output sampler_tx;

reg sample;
time current_time;
always @(A or B or buss)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

