begin
	buss[2] = 1'b1;
end 
// buss[ 1 ]
initial
begin
	buss[1] = 1'b0;
end 
// buss[ 0 ]
initial
begin
	buss[0] = 1'b1;
end 

ALU_vlg_sample_tst tb_sample (
	.A(A),
	.B(B),
	.buss(buss),
	.sampler_tx(sampler)
);

ALU_vlg_check_tst tb_out(
	.display(display),
	.led(led),
	.resultado(resultado),
	.sampler_rx(sampler)
);
endmodule

